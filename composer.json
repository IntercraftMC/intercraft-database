{
    "name": "intercraftmc/database",
    "description": "The official database schema for the Intercraft Minecraft servers",
    "type": "project",
    "homepage": "https://github.com/IntercraftMC/intercraft-website",
    "license": "MIT",
    "authors": [
        {
            "name": "David Ludwig",
            "email": "davidludwigii@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "fzaninotto/faker": "^1.8",
        "symfony/debug": "^4.3"
    },
    "require-dev": {
        "dyrynda/nomad": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "IntercraftDb\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "bin-dir": "./"
    }
}
